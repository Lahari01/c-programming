#include <stdio.h>

int main ()
{
  int m;
  char grade;
  printf ("enter the marks of the student");
  scanf ("%d", &m);
  if (m >= 85)
    grade = 'A';
  else if (m >= 70)
    grade = 'B';
  else if (m >= 55)
    grade = 'C';
  else if (m >= 40)
    grade = 'D';
  else
    grade = 'F';
  printf ("%c is the grade obtained by the student.", grade);
  return 0;
}
